    "formik": "^2.4.5",
    "knex": "^3.1.0",
    "nodemon": "^3.0.2",
    "objection": "^3.1.3",
    "pg": "^8.11.3",
    "styled-components": "^6.1.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
