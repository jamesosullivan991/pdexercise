{
  "name": "pdexercise",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "run-s dev:app",
    "dev:app": "next dev src/frontend",
    "dev:db": "docker run -e POSTGRES_PASSWORD=secret -p 5432:5432 -d postgres:14.1",
    "db:migrate:new": "npx knex migrate:make --knexfile src/backend/db/knexfile.ts",
    "db:migrate": "npx knex migrate:latest --knexfile src/backend/db/knexfile.ts",
    "db:rollback": "npx knex migrate:rollback --knexfile src/backend/db/knexfile.ts",
    "db:make": "func() { npx knex migrate:make \"$1\" --knexfile src/backend/db/knexfile.ts; }; func",
    "build": "next build src/frontend",
    "start": "next start src/frontend",
    "start:api": "node dist/backend/app.js",
    "lint": "npx eslint --ext .js --ext .jsx . --cache",
    "lint:fix": "npm run lint -- --fix",
    "test": "run-s test:unit",
    "test:unit": "jest . --runInBand"
  },
  "keywords": [],
